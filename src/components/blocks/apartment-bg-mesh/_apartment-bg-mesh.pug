.apartment-bg-mesh
  - var appartmentItem = ["Apartment №510", "Apartment №507", "Apartment №505"]; //Apartment №506
  - var n = 0;
  while n < 3
    .apartment.apartment_free.two-room-el
      .apartment-state.prom-p2-medium Free
      .apartment__title.prom-h1= appartmentItem[n]
      .apartment__descr 
        .p2 2к
        .p2 65.2 м²
        .p2 2 floor
      .apartment-slider.swiper-container 
        .apartment-slider__wrapper.swiper-wrapper 
          - var imagesContent = ["img/filtered-img.png", "img/filtered-img-1.png", "img/filtered-img-2.png", "img/filtered-img-3.png"];
          - var imagesSrc = ["img/filtered-img.png", "img/filtered-img-1.png", "img/filtered-img-2.png", "img/filtered-img-3.png"];
          - var k = 0;
          while k < 4
            a(href= imagesContent[k] data-fancybox="gallery").apartment-slider__el.swiper-slide 
              img(src= imagesSrc[k] alt="")
            - k++
        .control-inner
          .swiper-pagination
      .apartment__user 
        .apartment__user-price 
          .h2  10 281 180 ₽ 
          .prom-p2-medium  161 400 ₽/м² 
        .apartment__user-send 
          a(href="#").prom-b1  Submit request
      .apartment__info
        .apartment__info-descr 
          .prom-p2-medium
            span.prom-p2-small Apartment with finishing
            |Apartment with finishing
        .apartment__info-title.prom-p2-medium Characteristics
        .apartment__info-icon 
          img(src="data:image/png;base64," alt="")
        - var k = 0;
        while k < 10
          .apartment__info-el
            .p2 Address
            .p2 Obninsk - floor 2 № 505
          - k++
    - n++
  .apartment.apartment_free.studio-el
    .apartment-state.prom-p2-medium Free
    .apartment__title.prom-h1 Apartment №506
    .apartment__descr 
      .p2 2к
      .p2 65.2 м²
      .p2 2 floor
    .apartment-slider.swiper-container 
      .apartment-slider__wrapper.swiper-wrapper 
        - var imagesContent = ["img/filtered-img.png", "img/filtered-img-1.png", "img/filtered-img-2.png", "img/filtered-img-3.png"];
        - var imagesSrc = ["img/filtered-img.png", "img/filtered-img-1.png", "img/filtered-img-2.png", "img/filtered-img-3.png"];
        - var k = 0;
        while k < 4
          a(href= imagesContent[k] data-fancybox="gallery").apartment-slider__el.swiper-slide 
            img(src= imagesSrc[k] alt="")
          - k++
      .control-inner
        .swiper-pagination
    .apartment__user 
      .apartment__user-price 
        .h2  10 281 180 ₽ 
        .prom-p2-medium  161 400 ₽/м² 
      .apartment__user-send 
        a(href="#").prom-b1  Submit request
    .apartment__info
      .apartment__info-descr 
        .prom-p2-medium
          span.prom-p2-small Apartment with finishing
          |Apartment with finishing
      .apartment__info-title.prom-p2-medium Characteristics
      .apartment__info-icon 
        img(src="data:image/png;base64," alt="")
      - var k = 0;
      while k < 10
        .apartment__info-el
          .p2 Address
          .p2 Obninsk - floor 2 № 505
        - k++