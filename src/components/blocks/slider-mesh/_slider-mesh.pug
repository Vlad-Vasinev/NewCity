.slider-mesh
  .meshed-slider.swiper-container
    .swiper-wrapper
      - var texts = ["Q1 2021", "IQ1 2022", "IQ1 2023", "IQ1 2024"]
      - var imgSrc = ["img/meshed-slider-1.jpg", "img/meshed-slider-2.jpg", "img/meshed-slider-3.jpg", "img/meshed-slider-4.jpg"]
      - var n = 0;
      while n < 4
        .meshed-slider__item.swiper-slide
          a(href="#").meshed-slider__img(js-lazy) 
            .meshed-slider__img-count.p1 
              <span class="p1">10</span> photo
            img(src=imgSrc[n] alt="")
          .h3=texts[n]
        - n++
